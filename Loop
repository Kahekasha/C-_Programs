while (j % 2 == 0)
{
    Console.WriteLine("Even");
    j++;
}*/


using System;
using System.Globalization;

class program
{
    public static void Main(string[] args)
    {
        for (int i = 1; i <= 10; i++)
        {
            for(int j=i; j<=100; j=j+10)
            {


                Console.Write($"({j})");

            }
            Console.WriteLine("");

        }

    }
}


