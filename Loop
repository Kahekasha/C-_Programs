using System;
using System.Globalization;

class program
{
    public static void Main(string[] args)
    {
        for (int i = 1; i <=100; i=i+10)
        {
            for(int j=i; j<i+10; j++)
            {


                Console.Write($"({j})");

            }
            Console.WriteLine("");

        }

    }
}

